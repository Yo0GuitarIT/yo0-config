{
    "name": "@yo0-configs/prettier-config",
    "version": "1.0.0",
    "description": "Shared Prettier configuration",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "prettier",
        "config",
        "formatting"
    ],
    "peerDependencies": {
        "prettier": "^3.0.0"
    }
}
